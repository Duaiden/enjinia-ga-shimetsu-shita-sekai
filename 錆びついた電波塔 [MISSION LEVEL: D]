#include <stdio.h>

int readInt(){
    int temp;
    scanf("%d", &temp);
    return temp;
}

int main(void){
    int n;
    int cnt = 0;
    scanf("%d",&n);  
    
    for (int i = 0; i < n; i++) {
        if(readInt() > 5){
            cnt ++;
        }
    }
    
    printf("%d", cnt);
    
    return 0;
}
