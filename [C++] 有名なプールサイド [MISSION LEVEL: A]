#include <bits/stdc++.h>
#define _for(i,a,b) for(int i = (a),i##end=(b);i < i##end;i ++)
#define pb push_back
using namespace std;

int main()
{
    int H,W,N;
    while(~scanf("%d%d%d",&H,&W,&N))
    {
        int m[101][101];
        memset(m,0,sizeof(m));
        int flag = 0;
        _for(i,0,N)
        {
            int a1,b1,a2,b2;
            scanf("%d%d%d%d",&a1,&b1,&a2,&b2);
            int Ll = 0,Rl = 0,Tl = 0,Dl = 0;
            if(!flag&&a1<=H&&b1<=W)
            {
                flag = 1;
                if(a2==1)
                    Tl = 1;
                if(b2==1)
                    Ll = 1;
                if(a2==a1)
                    Dl = 1;
                if(b2==b1)
                    Rl = 1;
                
                if(Tl||Rl)
                {
                    _for(j,H-a1,H)
                        _for(k,0,b1)
                            m[j][k] = 1;
                }
                else if(Dl||Ll)
                {
                    _for(j,0,a1)
                        _for(k,W-b1,W)
                            m[j][k] = 1;
                }
            } 
        }
        _for(i,0,H)
        {
            _for(j,0,W)
            {
                cout << m[i][j];
                if(j!=W-1)
                    cout << " ";
            }
            if(i!=H-1)
                cout << endl;
        }
    }
    return 0;
}
